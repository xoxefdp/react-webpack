  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --mode development --watch --progress --open --host 0.0.0.0",
    "start:prod": "webpack-dev-server --mode production --watch --progress --open --host 0.0.0.0",
    "build:dev": "webpack --mode development --progress",
    "build:prod": "webpack --mode production --progress",
    "postinstall": "npm dedupe --loglevel verbose",
    "prebuild:dev": "npm run clean --loglevel verbose",
    "prebuild:prod": "npm run clean --loglevel verbose",
    "clean": "rimraf dist --loglevel verbose",
